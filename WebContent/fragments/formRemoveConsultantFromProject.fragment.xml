<core:FragmentDefinition 
	xmlns="sap.m" 
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
	xmlns:semantic="sap.m.semantic">
	
	
		<SelectDialog
		noDataText="No Consultants Found"
		title="Remove Consultants"
		search="handleSearch"
		confirm="handleCloseRemoveConsultantFromProject"
		cancel="onCancel"
		items="{membersModel>/results}" >
		
		<StandardListItem
			title="{membersModel>ConsultantDetails/Consultant_Name} {membersModel>ConsultantDetails/Consultant_Surname}"
			description="SAP programmer"
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
			
	</SelectDialog>
	
</core:FragmentDefinition>