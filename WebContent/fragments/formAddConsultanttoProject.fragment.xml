<core:FragmentDefinition 
	xmlns="sap.m" 
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
	xmlns:semantic="sap.m.semantic">
	
	
		<SelectDialog
		noDataText="No Consultants Found"
		title="Select Consultants"
		search="handleSearch"
		confirm="handleClose2"
		cancel="handleClose2"
		items="{consultants>/results}" >
		
		<StandardListItem
			title="{consultants>Consultant_Name} {consultants>Consultant_Surname}"
			description="{Role}"
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
			
	</SelectDialog>
	
</core:FragmentDefinition>