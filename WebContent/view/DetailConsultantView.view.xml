 <mvc:View xmlns:core="sap.ui.core" 
			xmlns:mvc="sap.ui.core.mvc" 
			xmlns="sap.m" 
			xmlns:m="sap.m"
			xmlns:semantic="sap.m.semantic" 
			xmlns:f="sap.ui.layout.form"
			xmlns:la="sap.ui.layout"
			xmlns:u="sap.ui.unified"
			xmlns:viz="sap.viz.ui5.controls"
			xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
			xmlns:viz.data="sap.viz.ui5.data"			 
			controllerName="consultanttracker.Consultant-Tracker_Prototype-1.controller.DetailConsultantView" 
			xmlns:html="http://www.w3.org/1999/xhtml">
		<semantic:DetailPage id="DetailConsultantView" navButtonPress="onNavBack" showNavButton="{device>/system/phone}"
		title="Consultant Detail" busy="{detailView>/busy}" busyIndicatorDelay="{detailView>/delay}">
<semantic:content>
			
<ObjectHeader id="objectHeader" title="{consultantsModel>/Consultant_Name}"></ObjectHeader>
			
			<IconTabBar id="iconTabBar" class="sapUiResponsiveContentPadding" expandable="false">
				<items>
					<IconTabFilter id="iconTabBarFilter1" icon="sap-icon://employee-pane" tooltip="{i18n>detailIconTabBarInfo}" text="Employee Details">
						<content>
		<ObjectHeader
		id="oh1"
		responsive="true"
		fullScreenOptimized="false"
		icon="{ProductPicUrl}"
		iconAlt="{Name}"
		intro="{projectsModel>/Project_Description}"
		title="{consultantsModel>/Consultant_Name} {consultantsModel>/Consultant_Surname}"
		titleActive="true"
		titlePress="handleTitlePress"
		number="34%"
		numberUnit="Completed"
		showTitleSelector="true"
		titleSelectorPress="onPress"
		showMarkers="true"
		markFlagged="true"
		markFavorite="true"
		numberState="Success"
		backgroundDesign="Translucent">
		<attributes>
			<ObjectAttribute
				title="Emp no."
				text="{consultantsModel>/Consultant_ID}"/>
			<ObjectAttribute
				title="Role"
				text="SAP Programer"/>
			<ObjectAttribute
				title="Name"
				text="{consultantsModel>/Consultant_Name}"/>
			<ObjectAttribute
				title="Contact"
				text="{consultantsModel>/Consultant_Cell}"/>
			<ObjectAttribute
				title="Email"
				text="{consultantsModel>/Consultant_email}"/>
		</attributes>
		<statuses>
 			<ObjectStatus
				title="Approval"
				text="Pending"
				state="Warning"/>
		</statuses>
	</ObjectHeader>
	
			</content>
			</IconTabFilter>
					
					
					<IconTabFilter id="iconTabBarFilter2" icon="sap-icon://folder"  tooltip="{i18n>detailIconTabBarAttachments}" text="Projects">
						<content>
						
							<List 	id="consultantProjects" 
									items="{consultantProjectsModel>/results}"
									noDataText="No projects available">
								
								<items>
										<ObjectListItem											
											title="Name: {consultantProjectsModel>ProjectDetails/Project_Name}" 
											type="Active"
											press= "handleOpenDialog" >
												<firstStatus>
												<ObjectStatus />
												</firstStatus>
												<attributes>
												<ObjectAttribute text="Date " icon="sap-icon://task" />
												</attributes>
										</ObjectListItem>
											
									
								</items>
							</List>
						</content>
					</IconTabFilter>
					<IconTabFilter id="iconTabBarFilter3" icon="sap-icon://kpi-corporate-performance" tooltip="List of tasks" text="Stats">


					</IconTabFilter>
<IconTabFilter id="iconTabBarFilter4" icon="sap-icon://line-chart-time-axis" tooltip="Utilization" text="Utilization" design="sap.m.IconTabFilterDesign.Vertical">
						<content>	
									<OverflowToolbar>
										<Title text="Utilization" level="H2"/>
										<ToolbarSpacer/>
										<Select id="utilizationMonthSelect" change="onUtilizationMonthChange" name="{consultantsModel>/Consultant_ID}">
											<core:Item text="January" id="month0" />
											<core:Item text="February" id="month1" />
											<core:Item text="March" id="month2" />
											<core:Item text="April" id="month3" />
											<core:Item text="May" id="month4" />
											<core:Item text="June" id="month5" />
											<core:Item text="July" id="month6" />
											<core:Item text="August" id="month7" />
											<core:Item text="September" id="month8" />
											<core:Item text="October" id="month9" />
											<core:Item text="November" id="month10" />
											<core:Item text="December" id="month11" />
											<core:Item text="All" id="monthAll" />
										</Select>
										<!-- <ToolbarSpacer/> -->
 										<Input id="utilizationYearInput" type="Number" maxlength="4" liveChange="onUtilizationYearLiveChange" width="10%"/>
									</OverflowToolbar>		
							<HBox justifyContent="sap.m.FlexJustifyContent.Center">									
					        <viz:VizFrame id="oVizFrame" busy="false" busyIndicatorDelay="1000" visible="true" uiConfig="{ 'applicationSet': 'fiori' }" vizType="pie" 
					        vizProperties="{ title: {visible:'false'}, plotArea:{dataLabel:{visible:'true'}} }" legendVisible="true">
								<viz:dataset>
									<viz.data:FlattenedDataset data="{utilization>/utilization}">
										<viz.data:dimensions>
											<viz.data:DimensionDefinition name="category" value="{category}"/>
										</viz.data:dimensions>
										<viz.data:measures>
											<viz.data:MeasureDefinition group="1" name="hours" value="{hours}"/>
											
										</viz.data:measures>
									</viz.data:FlattenedDataset>
								</viz:dataset>
								<!-- sap.viz.ui5.data.Dataset -->
								<viz:feeds>
									<viz.feeds:FeedItem id="value1" uid="size" type="Measure" values="hours"/>
									
									<viz.feeds:FeedItem uid="color" type="Dimension" values="category"/>
								</viz:feeds>
							</viz:VizFrame>	
							</HBox>
						</content>		
					</IconTabFilter>				
						
					<IconTabFilter id="iconTabBarFilter5" icon="sap-icon://favorite" tooltip="{i18n>detailIconTabBarAttachments}" text="Rating">

						<Table mode="SingleSelectMaster" selectionChange=""  growing="true" inset="false" items="{ratings>/results}">
							<headerToolbar>
								<OverflowToolbar>
									<Title text="Rating" level="H2"/>

								</OverflowToolbar>
							</headerToolbar>
							<columns>
								<Column width="12em" >
									<Text text="Project"/>
								</Column>
								<Column minScreenWidth="Tablet" demandPopin="true">
									<Text text="Rating"/>
								</Column>
								<Column minScreenWidth="Tablet" demandPopin="true">
									<Text text="Number of Voters"/>
								</Column>								
							</columns>
							<items>
								<ColumnListItem>
									<cells>
										<ObjectIdentifier text="{ratings>ProjectDetails/Project_Name}"/>
										<ProgressIndicator class="sapUiSmallMarginBottom" percentValue="{ratings>Rating}" displayValue="{ratings>Rating}" showValue="true" state="None"/>
										<Text renderWhitespace="true" text="\t {ratings>Num_Votes}"/>
										
									</cells>
							
								</ColumnListItem>
							</items>
						</Table>
					</IconTabFilter>						
					
				</items>
			</IconTabBar>
		</semantic:content>

		<semantic:customFooterContent>
			<Button id="__button0" text="Remove Project" icon="sap-icon://delete" press="confirmDeleteProject"/>
		</semantic:customFooterContent>
	</semantic:DetailPage>
</mvc:View>