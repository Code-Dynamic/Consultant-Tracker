<mvc:View  
xmlns:mvc="sap.ui.core.mvc" 
xmlns="sap.m"
xmlns:tnt="sap.tnt"
displayBlock="true"
controllerName="consultanttracker/Consultant-Tracker_Prototype-1/controller/SplitApp"
>
    
    <Page showHeader="false" enableScrolling="true">
     <tnt:ToolHeader>
        <Button text="Consultant Tracker" type="Transparent" press="onHomePress">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow"/>
            </layoutData>
        </Button>
        <IconTabHeader id="iconTabHeader" selectedKey="invalidKey" select="onSelectTab" showOverflowSelectList="true" backgroundDesign="Transparent">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" shrinkable="true"/>
            </layoutData>
        </IconTabHeader>
        <ToolbarSpacer>
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" minWidth="16px"/>
            </layoutData>
        </ToolbarSpacer>
        <MenuButton id="splitAppMenuButton" text="UserName" type="Transparent">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow"/>
            </layoutData>
            <menu>
                <Menu>
                    <items>
                        <MenuItem text="Profile"/>
                        <MenuItem text="Logout"/>
                    </items>
                </Menu>
            </menu>
        </MenuButton>
    </tnt:ToolHeader>
    
 <content>
 <SplitApp id="idSplitAppControl"/>
 </content>

 </Page>
</mvc:View>