 <mvc:View xmlns:core="sap.ui.core" 
			xmlns:mvc="sap.ui.core.mvc" 
			xmlns="sap.m" 
			xmlns:semantic="sap.m.semantic" 
			xmlns:f="sap.ui.layout.form"
			xmlns:la="sap.ui.layout"
			xmlns:u="sap.ui.unified" 
			controllerName="consultanttracker.Consultant-Tracker_Prototype-1.controller.DetailConsultant" 
			xmlns:html="http://www.w3.org/1999/xhtml">
		<semantic:DetailPage id="DetailConsultant" navButtonPress="onNavBack" showNavButton="{device>/system/phone}"
		title="Consultant Detail" busy="{detailView>/busy}" busyIndicatorDelay="{detailView>/delay}">
		<semantic:customHeaderContent>
			<Button press="openCalender" id="cartIcon" text="Schedule" icon="sap-icon://accelerated"/>
		</semantic:customHeaderContent>
<semantic:content>
			
<ObjectHeader id="objectHeader" title="{projectsModel>/Project_Name}">
			    <statuses>
			       <ProgressIndicator id="projectProgressId" class="sapUiSmallMarginBottom" percentValue="{progress>/percVal}" displayValue="{progress>/displayVal}%" showValue="true" state="None"/>

			    </statuses>
			</ObjectHeader>
			
			<IconTabBar id="iconTabBar" class="sapUiResponsiveContentPadding" expandable="false">
				<items>
					<IconTabFilter id="iconTabBarFilter1" icon="sap-icon://detail-view" tooltip="{i18n>detailIconTabBarInfo}" text="Project Data">
						<content>
								<ObjectHeader
		id="oh1"
		responsive="true"
		fullScreenOptimized="false"
		icon="{ProductPicUrl}"
		iconAlt="{Name}"
		intro="{projectsModel>/Project_Description}"
		title="{projectsModel>/Project_Name}"
		titleActive="true"
		titlePress="handleTitlePress"
		number="34%"
		numberUnit="Completed"
		showTitleSelector="true"
		titleSelectorPress="onPress"
		showMarkers="true"
		markFlagged="true"
		markFavorite="true"
		numberState="Success"
		backgroundDesign="Translucent">
		<attributes>
			<ObjectAttribute
				title="ProjectID"
				text="{projectsModel>/Project_ID}"/>
			<ObjectAttribute
				title="Due"
				text="{projectsModel>/Project_Deadline}"/>
			<ObjectAttribute
				title="Client Name"
				text="{projectsModel>/ClientDetails/Client_Name}"/>
			<ObjectAttribute
				title="Client Contact"
				text="{projectsModel>/ClientDetails/Client_PhoneNum}"/>
			<ObjectAttribute
				title="Dimension per unit"
				text="{Width} x {Depth} x {Height} {DimUnit}"/>
		</attributes>
		<statuses>
 			<ObjectStatus
				title="Approval"
				text="Pending"
				state="Warning"/>
		</statuses>
	</ObjectHeader>
	
			</content>
			</IconTabFilter>
					
					
					<IconTabFilter id="iconTabBarFilter2" icon="sap-icon://group" tooltip="{i18n>detailIconTabBarAttachments}" text="Members">
						<content>
							<List 	id="members" 
									mode="SingleSelectMaster" 
									items="{membersModel>/results}" 
									headerText="Members">
								
								<items>
									<StandardListItem 
											title="{membersModel>/ProjectDetails/Project_ID} Member Name" 
											type="Active" 
											class="listItems" 
											press=""/>
								</items>
							</List>
						</content>
					</IconTabFilter>
					<IconTabFilter id="iconTabBarFilter3" icon="sap-icon://activities" tooltip="{i18n>detailIconTabBarAttachments}" text="Tasks">

						<Table mode="SingleSelectMaster" selectionChange="onSelectionChange" id="idProgressTable" growing="true" inset="false" items="{tasks>/results}">
							<headerToolbar>
								<OverflowToolbar id="otbSubheader">
									<Title text="Tasks" level="H2"/>
									<ToolbarSpacer/>
									<Button id="AddTask" text="Add Task" type="Transparent" press="onAddTask"/>

								</OverflowToolbar>
							</headerToolbar>
							<columns>
								<Column width="12em">
									<Text text="Task Name"/>
								</Column>
								<Column minScreenWidth="Tablet" demandPopin="true">
									<Text text="Deadline"/>
								</Column>
								<Column minScreenWidth="Tablet" demandPopin="true">
									<Text text="Progress"/>
								</Column>
								<Column width="80px" minScreenWidth="Tablet" demandPopin="true">
									<Text text="{}"/>
								</Column>
								<Column width="150px" minScreenWidth="Tablet" demandPopin="true">
									<Text text="{}"/>
								</Column>
								<Column width="50px" minScreenWidth="Tablet" demandPopin="true">
									<Text text="{}"/>
								</Column>
							</columns>

							<items>
								<ColumnListItem>
									<cells>
										<ObjectIdentifier text="{tasks>Name}"/>
										<Text text="{tasks>Due_Date}"/>
										<ProgressIndicator class="sapUiSmallMarginBottom" percentValue="30" displayValue="30%" showValue="true" state="None"/>

										<!--  <MenuButton text="More" buttonMode="Split"
											useDefaultActionOnly="true">
											<menu>
												<Menu itemSelected="onMenuAction">
													<items>
														<MenuItem text="Feedback" id="feedback" />
														<MenuItem text="Task Detail" />
														<MenuItem text="Manage Consultant" />
													</items>
												</Menu>
											</menu>

										</MenuButton>

										<Button type="Reject" icon="sap-icon://delete" press="onPress" /> -->
										<Link text="Feedback" press="getFeedback"/> 
										<Link text="Manage Consultant" press="openManageConFrag"/>
										<!--<Link text="Delete" press="handleLinkPress" /> -->
										<Button type="Reject" icon="sap-icon://delete" press="deleteTask"/>
										
										
									</cells>
								</ColumnListItem>
							</items>
						</Table>

					</IconTabFilter>
					<IconTabFilter id="iconTabBarFilter4" icon="sap-icon://attachment" tooltip="{i18n>detailIconTabBarInfo}" text="Attachments">
						<content>
							<la:VerticalLayout>
								<u:FileUploader id="fileUploader" name="myFileUpload" uploadUrl="AddFolder" tooltip="Upload your file to the local server" uploadComplete="" change="handleValueChange" typeMissmatch="handleTypeMissmatch" style="Emphasized" fileType="txt,jpg" placeholder="Choose a file for Upload...">
									<u:parameters>
										<u:FileUploaderParameter name="param1" value="Foo"/>
										<u:FileUploaderParameter name="param2" value="bar"/>
									</u:parameters>
									<u:headerParameters>
										<u:FileUploaderParameter name="headerParam1" value="Foo"/>
										<u:FileUploaderParameter name="headerParam2" value="bar"/>
									</u:headerParameters>
								</u:FileUploader>
								<Button text="Upload File" press="handleUploadPress"/>
							</la:VerticalLayout>

							<Table id="idTable" items="{attachment>/results}">
								<items>
									<ColumnListItem>
										<cells>
											<Text text="{attachment>Attachment_Name}"/>
											<Text text="{attachment>Attachment_Upload_Date}"/>
											<Text text="{attachment>Attachment_Size} B"/>
										</cells> 
									</ColumnListItem>
								</items>
								<columns>
									<Column>
										<Text text="File Name"/>
									</Column>
									<Column>
										<Text text="Date Uploaded"/>
									</Column>
									<Column>
										<Text text="Size"/>
									</Column>
									
								</columns>
							</Table>
						</content>
					</IconTabFilter>

				</items>
			</IconTabBar>
		</semantic:content>

		<semantic:customFooterContent>
			<Button id="__button0" text="Remove Project" icon="sap-icon://delete" press="confirmDeleteProject"/>
		</semantic:customFooterContent>
	</semantic:DetailPage>
</mvc:View>