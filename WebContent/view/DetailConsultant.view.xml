 <mvc:View xmlns:core="sap.ui.core" 
			xmlns:mvc="sap.ui.core.mvc" 
			xmlns="sap.m" 
			xmlns:m="sap.m"
			xmlns:semantic="sap.m.semantic" 
			xmlns:f="sap.ui.layout.form"
			xmlns:l="sap.ui.layout"
			xmlns:la="sap.ui.layout"
			xmlns:mc="sap.suite.ui.microchart"
			xmlns:suite="sap.suite.ui.commons"
			xmlns:u="sap.ui.unified" 
			xmlns:viz="sap.viz.ui5.controls"
			xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
			xmlns:viz.data="sap.viz.ui5.data"
			xmlns:microchart="sap.suite.ui.microchart"
			xmlns:vbm="sap.ui.vbm"
			controllerName="consultanttracker.Consultant-Tracker_Prototype-1.controller.DetailConsultant" 
			xmlns:html="http://www.w3.org/1999/xhtml"
			xmlns:sample="sap.uxap.sample.ObjectPageOnJSON"
		   xmlns:goals="sap.uxap.sample.SharedBlocks.goals"
		   xmlns:personal="sap.uxap.sample.SharedBlocks.personal"
		   xmlns:employment="sap.uxap.sample.SharedBlocks.employment">
			
	<semantic:DetailPage 
			navButtonPress="onNavBack" 
			showNavButton="{device>/system/phone}" 
			busy="{detailView>/busy}" 
			busyIndicatorDelay="{detailView>/delay}">

		<semantic:customHeaderContent>
			
		</semantic:customHeaderContent>
		
<semantic:content>
			

		<ObjectHeader

		id="oh1"
		navButtonPress="onNavBack" 
		showNavButton="{device>/system/phone}"
		responsive="true"
		fullScreenOptimized="false"
		icon="{ProductPicUrl}"
		iconAlt="{Name}"
		intro="{projectsModel>/Project_Description}"
		title="{projectsModel>/Project_Name}"
		numberState="Success"
		backgroundDesign="Transparent">

		<attributes>

			
			<ObjectAttribute
				title="Due"
				text="{ path: 
							'projectsModel>/Project_Deadline',
							formatter: '.formatter.date'}"/>
					
			<!-- <ObjectAttribute
				title="Client Name: "
				text="{projectsModel>/ClientDetails/Client_Name}"/> -->
				
		</attributes>

	</ObjectHeader>

			
			<IconTabBar 
			id="iconTabBar"  
			class="sapUiResponsiveContentPadding" 
			expandable="false"
			select="showMap">
			
			<items>

			<IconTabFilter 
				id="iconTabBarFilter1"
				key="key1" 
				icon="sap-icon://hint" 
				tooltip="Dashboard" 
				text="Dashboard">
					
			<content>

			<GenericTile
			id="1" 
			class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Overall progress" 
			subheader="Current">
			
			<TileContent >

						<mc:RadialMicroChart percentage="{projectsModel>/projectProgress}" />	
			
			</TileContent>
		
			</GenericTile>
			
			<GenericTile 
			id="2"
			class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Consultants on Project" 
			subheader="Available" 
			press="onSelectTab">
			
			<TileContent >
			
			<NumericContent 
			value="{membersModel>/countMembers}" 
			icon="sap-icon://employee" />
			
			</TileContent>
		
			</GenericTile>
			
			<GenericTile 
			id="3"
			class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Active Tasks" 
			subheader="in progress"
			press="onSelectTab">
			
			<TileContent >
			
			<NumericContent 
			value="{projectsModel>/countTasks}" 
			icon="sap-icon://activities"/>
	
			</TileContent>
		
			</GenericTile>
			
			
			<GenericTile 
			id="4"
			class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Overall hours worked" 
			subheader="current" >
			
			<TileContent 
			unit="{projectsModel>/countExpectedHours}" 
			footer="Expected hours">
			
			<NumericContent 
			value="{projectsModel>/countHoursWorked}" 
			icon="sap-icon://present"/>
			
			</TileContent>
		
			</GenericTile>
			
			
			<GenericTile 
			id="6"
			class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Calender" 
			subheader="view schedule" 
			press="openCalender" >
			
			<TileContent>
			
			<NumericContent 
			value="{projectsModel>/countTasks}" 
			icon="sap-icon://accelerated"/>
			
			</TileContent>
		
			</GenericTile>
			
			<GenericTile 
			id="7"
			class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Show Route to" 
			subheader="{projectsModel>/ClientDetails/Client_Name}" 
			press="onSelectTab" >
			
			<TileContent>
				<NumericContent 
			icon="sap-icon://map"/>
			</TileContent>
		
			</GenericTile>
		
			</content>
						
					</IconTabFilter>
					
					
					<IconTabFilter 
					id="iconTabBarFilter2"
					key="key2"  
					icon="sap-icon://group" 
					tooltip="Team" 
					text="Team">
					
						<content>
						
<Table 
						id="teamMemberTable"
						inset="false"
						items="{membersModel>/results}">
		
						<headerToolbar>
		
							<Toolbar>
			
								<content>
		
									
				</content>
				
			</Toolbar>
			
		</headerToolbar>
		
		<columns>
		
			<Column
				width="12em">
				
				<Text 
					text="Name"/>
					
			</Column>
			
			<Column
				minScreenWidth="Tablet"
				demandPopin="true">
				
				<Text 
					text="Email" />
					
			</Column>

			<Column
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="Center">
				
				<Text 
					text="Role" />
					
			</Column>
			
				</columns>
				
		<items>
		
			<ColumnListItem>
			
				<cells>
				

					<Text
						text="{membersModel>ConsultantDetails/Consultant_Name}" />
						
					<Text
						text="{membersModel>ConsultantDetails/Consultant_email}" />
						
					<Text
						text="Programmer" />

				</cells>
				
			</ColumnListItem>
			
		</items>
		
	</Table>
	
	
	
							
						</content>
						
					</IconTabFilter>
					
					<IconTabFilter 
					id="iconTabBarFilter3"
					key="key3" 
					icon="sap-icon://activities"
					tooltip="Tasks" 
					text="Tasks">

		<Table id="idProductsTable"
		inset="false"
		selectionChange = "rowSelect"
		mode= "SingleSelectMaster"
		items="{tasksModel>/results}">
		
		<headerToolbar>
		
			<Toolbar>
			
				<content>
				
					
					<ToolbarSpacer />
					

					
				</content>
				
			</Toolbar>
			
		</headerToolbar>
		
		<columns>
		
			<Column
				width="12em">
				<Text text="Name"/>
			</Column>
			
			<Column
				minScreenWidth="Tablet"
				demandPopin="true">
				<Text text="Due date" />
			</Column>
			
			<Column
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="End">
				<Text text="Progress" />
			</Column>
			
			<Column
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="Center">
				<Text text="Status" />
			</Column>

		</columns>
		
		
		<items>
		
			<ColumnListItem>
			
				<cells>
				
				<ObjectIdentifier
				title="{tasksModel>Name}"/>
						
				<Text
				text="{ path: 
					 			'tasksModel>Due_Date',
								formatter: '.formatter.date'}" />
						

				<ProgressIndicator
				class="sapUiSmallMarginBottom"
				percentValue="{tasksModel>progress}"
				displayValue="{tasksModel>progress} %"
				showValue="true"
				state="None"
				displayOnly="false" />
						
				<Text text="{tasksModel>status}" />

				</cells>
				
			</ColumnListItem>
			
		</items>
		
	</Table>

					</IconTabFilter>
					
					<IconTabFilter 
					id="iconTabBarFilter4"
					key="key4" 
					icon="sap-icon://business-card"
					tooltip="Client details" 
					text="Client">
						<content id='contentDiv'>
						
						<QuickViewPage>
						<QuickViewGroup heading="Contact Person">
							
							<QuickViewGroupElement
									label="Name "
									value="{projectsModel>/ClientDetails/Client_Name}"
									url="url"
									type="elementType"
									pageLinkId="pageLinkId"
									emailSubject="emailSubject"
									target="target">
							</QuickViewGroupElement>
									
							<QuickViewGroupElement
									label="Email "
									value="{projectsModel>/ClientDetails/Client_EMail}"
									url="url"
									type="elementType"
									pageLinkId="pageLinkId"
									emailSubject="emailSubject"
									target="target">
							</QuickViewGroupElement>
							
							<QuickViewGroupElement
									label="Mobile "
									value="{projectsModel>/ClientDetails/Client_PhoneNum}"
									url="url"
									type="elementType"
									pageLinkId="pageLinkId"
									emailSubject="emailSubject"
									target="target">
							</QuickViewGroupElement>
														
							</QuickViewGroup>
							<QuickViewGroup heading="Company Details">
							
							<QuickViewGroupElement
									label="Company Name "
									value="{projectsModel>/ClientDetails/Company_name}"
									url="url"
									type="elementType"
									pageLinkId="pageLinkId"
									emailSubject="emailSubject"
									target="target">
							</QuickViewGroupElement>
							
							<QuickViewGroupElement
									label="Phone "
									value="{projectsModel>/ClientDetails/Company_phone}"
									url="url"
									type="elementType"
									pageLinkId="pageLinkId"
									emailSubject="emailSubject"
									target="target">
							</QuickViewGroupElement>
									
							<QuickViewGroupElement
									label="Address "
									value="{projectsModel>/ClientDetails/Client_Address}"
									url="url"
									type="elementType"
									pageLinkId="pageLinkId"
									emailSubject="emailSubject"
									target="target">
							</QuickViewGroupElement>
														
							</QuickViewGroup>
							
							
							
							</QuickViewPage>
							</content>

					</IconTabFilter>
					
					<IconTabFilter 
						id="iconTabBarFilter5"
						key="key5" 
						icon="sap-icon://map"
						tooltip="Map" 
						text="Map"  >

						<content id='contentDiv'>
						

						 	<HBox>
								 <Button press="ShowHide" text="Show/Hide Route"/>
							</HBox>
							 <Text text="Distance:" id="Distance" />
								
							<HBox id="map_canvas" fitContainer="true" justifyContent="Center" alignItems="Center"   />
						</content>

					</IconTabFilter>
					
				</items>
				
			</IconTabBar>
			
		</semantic:content>

		<semantic:customFooterContent>
					<Button						
						id="rateTeamBtn" 
 						text="Team Member Rating" 
 						icon="sap-icon://favorite" 
 						press="onRateTeam" 
 						enabled="false" 
 						visible="false" />				

				
		</semantic:customFooterContent>
	</semantic:DetailPage>
	<Dialog 
		title="Rate Team Members" 
		id="ratingsDialog">
	
		<beginButton>
		
		<Button 
			text="Submit" 
			press="onSubmitRates" 
			id="submitRatesButton"/>
		</beginButton>
		
		<endButton>
		<Button 
			text="Close" 
			press="onRatingsDialogClose" 
			id="closeRatesButton"/>
		</endButton>
		
	</Dialog>	
</mvc:View>